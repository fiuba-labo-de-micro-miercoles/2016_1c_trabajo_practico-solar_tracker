00011d 34a0      		CPI		XL,LOW(TX_BUF+BUF_SIZE)
00011e f020      		BRLO	SALVA_PTR_TX
00011f 30b1      		CPI		XH,HIGH(TX_BUF+BUF_SIZE)
000120 f010      		BRLO	SALVA_PTR_TX
