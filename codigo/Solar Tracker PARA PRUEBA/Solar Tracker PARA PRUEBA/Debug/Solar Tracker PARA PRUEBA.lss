000115 34a0      		CPI		XL,LOW(TX_BUF+BUF_SIZE)
000116 f020      		BRLO	SALVA_PTR_TX
000117 30b1      		CPI		XH,HIGH(TX_BUF+BUF_SIZE)
000118 f010      		BRLO	SALVA_PTR_TX
