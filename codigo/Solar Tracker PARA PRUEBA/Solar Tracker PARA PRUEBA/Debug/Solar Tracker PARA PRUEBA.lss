000117 20aa      		TST		BYTES_A_TX	; hay datos pendientes de transmisin?
000118 f069      		BREQ	FIN_TRANSMISION
                 
000119 01d4      		MOVW	XL,PTR_TX_L	; Recupera puntero al prximo byte a tx.
00011a 910d      		LD		AUX,X+		; lee byte del buffer y apunta al
                +
                +
                +.if UDR0 < 0x40
                +out UDR0 , AUX
00011b 9300 00c6+.else 
                +sts UDR0 , AUX
