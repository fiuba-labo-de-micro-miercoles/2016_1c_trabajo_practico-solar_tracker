000119 e0a0     +ldi XL , LOW ( TX_BUF )
00011a e0b1     +ldi XH , HIGH ( TX_BUF )
                 		LDIW	X,TX_BUF	; ptr_tx=ptr_tx+1, (mdulo BUF_SIZE)
                 
                 SALVA_PTR_TX:
00011b 014d      		MOVW	PTR_TX_L,XL	; preserva puntero a sgte. dato
                 
00011c 94aa      		DEC		BYTES_A_TX	; Descuenta el nro. de bytes a tx. en 1
