00010f 20aa      		TST		BYTES_A_TX	; hay datos pendientes de transmisin?
000110 f069      		BREQ	FIN_TRANSMISION
                 
000111 01d4      		MOVW	XL,PTR_TX_L	; Recupera puntero al prximo byte a tx.
000112 910d      		LD		AUX,X+		; lee byte del buffer y apunta al
                +
                +
