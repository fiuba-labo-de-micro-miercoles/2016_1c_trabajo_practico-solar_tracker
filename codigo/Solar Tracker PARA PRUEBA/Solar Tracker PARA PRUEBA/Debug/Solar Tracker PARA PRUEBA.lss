                 SALVA_PTR_TX:
000123 014d      		MOVW	PTR_TX_L,XL	; preserva puntero a sgte. dato
                 
000124 94aa      		DEC		BYTES_A_TX	; Descuenta el nro. de bytes a tx. en 1
